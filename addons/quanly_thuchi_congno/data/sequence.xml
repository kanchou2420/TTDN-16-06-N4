<odoo>
    <record id="seq_phieu_thu" model="ir.sequence">
        <field name="name">Phiếu Thu</field>
        <field name="code">phieu.thu</field>
        <field name="prefix">PT/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_phieu_chi" model="ir.sequence">
        <field name="name">Phiếu Chi</field>
        <field name="code">phieu.chi</field>
        <field name="prefix">PC/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_cong_no_phai_thu" model="ir.sequence">
        <field name="name">Công nợ phải thu</field>
        <field name="code">cong.no.phai.thu</field>
        <field name="prefix">CNT/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_cong_no_phai_tra" model="ir.sequence">
        <field name="name">Công nợ phải trả</field>
        <field name="code">cong.no.phai.tra</field>
        <field name="prefix">CNTR/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>